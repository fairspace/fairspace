###########################
###   Property shapes   ###
###########################

pfizer:LabelPropertyShape
    a sh:PropertyShape ;
    sh:path rdfs:label ;
    dash:singleLine true ;
    sh:datatype xsd:string ;
    sh:order 0 ;
    sh:description "The unique label of this item." ; # TO DO
    sh:maxCount 1 ;
    sh:name "Label" ;
    fs:importantProperty true ;
.
pfizer:IdentifierPropertyShape
    a sh:PropertyShape ;
    sh:path dcterms:identifier ;
    sh:datatype xsd:string ;
    sh:description "The identifier of this item." ; # TO DO
    sh:maxCount 1 ;
    sh:order 1 ;
    sh:name "Identifier" ;
.
pfizer:TitlePropertyShape
    a sh:PropertyShape ;
    sh:path dcterms:title ;
    sh:datatype xsd:string ;
    sh:order 3 ;
    sh:description "The title of this item." ; # TO DO
    sh:maxCount 1 ;
    sh:name "Title" ;
.
