fnscloud:Sample
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:description "A biological sample taken from a subject that can potentially be processed further to acquire data from. A multitude of samples can be taken from one or more subjects when for example performing time-scale experiments or sampling from different regions of the subject." ;
  sh:name "Sample" ;
  sh:ignoredProperties (
      rdf:type
      owl:sameAs
    ) ;
  sh:property fnscloud:LabelPropertyShape ;
  sh:property fnscloud:IdentifierPropertyShape ;
  sh:property fnscloud:IsDefinedByPropertyShape ;
  sh:property
  [
      sh:name "Secondary identifier" ;
      sh:description "Other identifier used for this sample." ;
      sh:datatype xsd:string ;
      sh:path fnscloud:secondarySampleIdentifier ;
  ] ;
  sh:property
  [
        sh:name "Host species" ;
        sh:description "The species of the sample host." ;
        sh:maxCount 1 ;
        sh:class fnscloud:Species ;
        sh:path fnscloud:hasHostSpecies ;
  ] ;
  sh:property [
        sh:name "Investigation type" ;
        sh:description "Investigation type" ;
        sh:class fnscloud:investigationType ;
        sh:path fnscloud:hasInvestigationType ;
    ] ;
  sh:property [
        sh:name "Host diet" ;
        sh:description "Host diet" ;
        sh:class fnscloud:diet ;
        sh:path fnscloud:hasHostDiet ;
    ] ;
  sh:property [
        sh:name "Special diet" ;
        sh:description "Special diet" ;
        sh:class fnscloud:diet ;
        sh:path fnscloud:hasSpecialDiet ;
    ] ;
  sh:property [
        sh:name "Environment biome" ;
        sh:description "Environment biome" ;
        sh:class fnscloud:environment ;
        sh:path fnscloud:hasEnvironmentBiome ;
    ] ;
  sh:property [
        sh:name "Environment feature" ;
        sh:description "Environment feature" ;
        sh:class fnscloud:environment ;
        sh:path fnscloud:hasEnvironmentFeature ;
    ] ;
  sh:property [
      sh:name "Location latitude" ;
      sh:description "Location latitude" ;
      sh:datatype xsd:decimal ;
      sh:maxCount 1 ;
      sh:path fnscloud:hasLocationLatitude ;
    ] ;
  sh:property [
      sh:name "Location longitude" ;
      sh:description "Location longitude" ;
      sh:datatype xsd:decimal ;
      sh:maxCount 1 ;
      sh:path fnscloud:hasLocationLongitude ;
    ] ;
  sh:property [
        sh:name "Sequencing method" ;
        sh:description "Sequencing method" ;
        sh:maxCount 1 ;
        sh:class fnscloud:sequencingMethod ;
        sh:path fnscloud:hasSequencingMethod ;
    ] ;
  sh:property [
        sh:name "Biosample id" ;
        sh:description "Biosample id" ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:path fnscloud:bioSampleId ;
    ] ;
  sh:property [
        sh:name "Analysis file" ;
        sh:description "Url to the related analysis file" ;
        sh:datatype xsd:string ;
        sh:path fnscloud:hasAnalysisFile ;
    ] ;
  sh:property [
      sh:path fnscloud:belongsToSubject ;
      sh:class fnscloud:Subject ;
      sh:description "The subject this sample belongs to." ;
      sh:maxCount 1 ;
      sh:name "Subject" ;
    ] ;
  sh:property [
      sh:path fnscloud:isSamplePartOfStudy ;
      sh:class fnsharmony:study ;
      sh:description "The study this sample belongs to." ;
      sh:name "Study" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:aboutSample ;
        ] ;
      sh:description "Linked files" ;
      sh:name "Files" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:sample ;
        ] ;
      sh:description "Linked assays" ;
      sh:name "Assays" ;
    ] ;
.

fnscloud:isAssayPartOfStudy a rdf:Property .

fnscloud:Assay
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:description "An assay is an investigative (analytic) procedure in laboratory medicine, mining, pharmacology, environmental biology and molecular biology for qualitatively assessing or quantitatively measuring the presence, amount, or functional activity of a target entity (the analyte)." ;
  sh:name "Assay" ;
  sh:ignoredProperties (
      rdf:type
      owl:sameAs
    ) ;
  sh:property fnscloud:LabelPropertyShape ;
  sh:property fnscloud:IdentifierPropertyShape ;
  sh:property fnscloud:IsDefinedByPropertyShape ;
  sh:property fnscloud:DescriptionPropertyShape ;
  sh:property [
      sh:path fnscloud:isAssayPartOfStudy ;
      sh:class fnsharmony:study ;
      sh:description "The study this assay belongs to." ;
      sh:name "Study" ;
    ] ;
  sh:property [
      sh:path fnscloud:hasFeature ;
      sh:class fnscloud:Feature ;
      sh:description "The feature of this assay." ;
      sh:name "Feature" ;
    ] ;
  sh:property [
      sh:path fnscloud:sample ;
      sh:class fnscloud:Sample ;
      sh:description "The sample this assay belongs to." ;
      sh:name "Sample" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:aboutAssay ;
        ] ;
      sh:description "Linked files" ;
      sh:name "Files" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:assay ;
        ] ;
      sh:description "Linked features" ;
      sh:name "Features" ;
    ] ;
.

fnscloud:assay
  rdf:type rdf:Property ;
.
fnscloud:hasFeature
  rdf:type rdf:Property ;
.

fnscloud:hasUnit a rdf:Property .

fnscloud:Feature
  rdf:type rdfs:Class ;
