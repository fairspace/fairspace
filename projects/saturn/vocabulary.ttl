# imports: http://www.w3.org/ns/shacl#

@prefix dash: <http://datashapes.org/dash#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fnscloud: <https://fns-cloud.eu/ontology/dbnp-data-model#> .
@prefix fnsharmony: <https://www.fns-cloud.eu/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix fs: <https://fairspace.nl/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://www.schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


fnscloud:aboutSample rdf:type rdf:Property .
fnscloud:aboutStudy rdf:type rdf:Property .
fnscloud:aboutGroup rdf:type rdf:Property .
fnscloud:aboutSubject rdf:type rdf:Property .
fnscloud:aboutAssay rdf:type rdf:Property .
fnscloud:aboutFeature rdf:type rdf:Property .

fs:Collection
  sh:property [
      sh:path fnscloud:aboutStudy ;
      sh:class fnsharmony:study ;
      sh:description "Studies that are featured in this collection." ;
      sh:name "Is about study" ;
    ] ;
.
fs:Directory
  sh:property [
      sh:path fnscloud:aboutStudy ;
      sh:class fnsharmony:study ;
      sh:description "Studies that are featured in this collection." ;
      sh:name "Is about study" ;
    ] ;
.
fs:File
  sh:property [
      sh:path fnscloud:aboutSample ;
      sh:class fnscloud:Sample ;
      sh:description "Samples that are featured in this collection." ;
      sh:name "Is about sample" ;
    ] ;
  sh:property [
      sh:path fnscloud:aboutStudy ;
      sh:class fnsharmony:study ;
      sh:description "Studies that are featured in this collection." ;
      sh:name "Is about study" ;
    ] ;
  sh:property [
      sh:path fnscloud:aboutGroup ;
      sh:class fnscloud:Group ;
      sh:description "Groups that are featured in this collection." ;
      sh:name "Is about group" ;
    ] ;
  sh:property [
      sh:path fnscloud:aboutSubject ;
      sh:class fnscloud:Subject ;
      sh:description "Subjects that are featured in this collection." ;
      sh:name "Is about subject" ;
    ] ;
  sh:property [
      sh:path fnscloud:aboutFeature ;
      sh:class fnscloud:Feature ;
      sh:description "Features that are featured in this collection." ;
      sh:name "Is about feature" ;
    ] ;
.
fnscloud:AnalysisFile
  sh:property [
      sh:path fnscloud:aboutStudy ;
      sh:class fnsharmony:study ;
      sh:description "Studies that are featured in this collection." ;
      sh:name "Is about study" ;
    ] ;
    sh:property fnscloud:LabelPropertyShape ;
    sh:property [
        sh:name "Location" ;
        sh:description "Location of an analysis file" ;
        sh:datatype xsd:string ;
        sh:path fnscloud:hasLocation ;
    ] ;
.

fnscloud:LabelPropertyShape
      a sh:PropertyShape ;
    sh:path rdfs:label ;
    dash:singleLine true ;
    sh:datatype xsd:string ;
    sh:order 0 ;
    sh:description "The unique label of this item." ;
    sh:maxCount 1 ;
    sh:name "Label" ;
    fs:importantProperty true ;
.
fnscloud:DescriptionPropertyShape
    a sh:PropertyShape ;
    sh:path dcterms:description ;
    sh:datatype xsd:string ;
    sh:order 6 ;
    sh:description "The description of this item." ;
    sh:name "Description" ;
.
fnscloud:StudyTypePropertyShape
    a sh:PropertyShape ;
    sh:path fnsharmony:has_study_type ;
    sh:class fnsharmony:study_type ;
    sh:description "The type of study, e.g. Human Intervention." ;
    sh:maxCount 1 ;
    sh:order 3 ;
    sh:name "StudyType" ;
.
fnscloud:TreatmentTypePropertyShape
    a sh:PropertyShape ;
    sh:path fnscloud:hasTreatmentType ;
    sh:class fnscloud:treatmentType ;
    sh:description "The type of treatment, e.g. fasting, OLTT." ;
    sh:name "TreatmentType" ;
.
fnscloud:SampleTypePropertyShape
    a sh:PropertyShape ;
    sh:path fnscloud:hasSampleType ;
    sh:class fnscloud:sampleType ;
    sh:description "Sample type, e.g. Organ/tissue Lower lobe lung, HDL." ;
    sh:name "SampleType" ;
.
fnscloud:IdentifierPropertyShape
    a sh:PropertyShape ;
    sh:path dcterms:identifier ;
    sh:datatype xsd:string ;
    sh:description "The identifier of this item." ;
    sh:maxCount 1 ;
    sh:order 1 ;
    sh:name "Identifier" ;
.
fnscloud:TitlePropertyShape
    a sh:PropertyShape ;
    sh:path dcterms:title ;
    sh:datatype xsd:string ;
    sh:order 5 ;
    sh:description "The title of this item." ;
    sh:maxCount 1 ;
    sh:name "Title" ;
.

fnscloud:GenderPropertyShape
    a sh:PropertyShape ;
        sh:path fnscloud:isOfGender ;
        sh:class fnscloud:Gender ;
        sh:description "The gender of the subject." ;
        sh:name "Gender" ;
        sh:maxCount 1 ;
.
fnscloud:IsDefinedByPropertyShape
    a sh:PropertyShape ;
    sh:path rdfs:isDefinedBy ;
    sh:order 2 ;
    fs:externalLink true ;
    sh:description "The source that defines this item" ;
    sh:name "Defined by (source)" ;
.

fnsharmony:study_type a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "The type of study, e.g. Human Intervention." ;
    sh:name "Study type" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

fnscloud:treatmentType a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Treatment type, e.g. screening, diclofenac, placebo, etc" ;
    sh:name "Treatment type" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

fnscloud:sampleType a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Sample type, e.g. Organ/tissue Lower lobe lung, HDL" ;
    sh:name "Sample type" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

dcterms:title rdf:type rdf:Property .
dcterms:description rdf:type rdf:Property .
dcterms:identifier rdf:type rdf:Property .
fnsharmony:has_study_type rdf:type rdf:Property .
fnscloud:hasTreatmentType rdf:type rdf:Property .
rdfs:isDefinedBy rdf:type rdf:Property .
fnscloud:subjectCount a rdf:Property .
fnscloud:authors a rdf:Property .
fnscloud:pubmedId a rdf:Property .
fnscloud:pubmedCentralId a rdf:Property .
fnscloud:publishedYear a rdf:Property .
fnscloud:isEnaDataAvailable a rdf:Property .
fnscloud:isMgnifyDataAvailable a rdf:Property .
fnscloud:isFoodIntakeAvailable a rdf:Property .
fnscloud:hasFoodIntakeFile a rdf:Property .
fnscloud:isGlucoseMonitorDoiAvailable a rdf:Property .
fnscloud:hasGlucoseMonitorDoi a rdf:Property .
fnscloud:isNutritionalIntakeAvailable a rdf:Property .
fnscloud:hasNutritionalIntakeFile a rdf:Property .
fnscloud:hasSampleType rdf:type rdf:Property .
fnscloud:secondaryStudyIdentifier rdf:type rdf:Property .

fnsharmony:study
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:description "A study (or experiment) comprises a series of subjects and assays (or measurements) of one or more types, undertaken to answer a particular biological question." ;
  sh:ignoredProperties (
      rdf:type
      owl:sameAs
    ) ;
  sh:name "Study" ;
  sh:property fnscloud:DescriptionPropertyShape ;
  sh:property fnscloud:IdentifierPropertyShape ;
  sh:property fnscloud:TitlePropertyShape ;
  sh:property fnscloud:LabelPropertyShape ;
  sh:property fnscloud:IsDefinedByPropertyShape ;
  sh:property fnscloud:StudyTypePropertyShape ;
  sh:property fnscloud:TreatmentTypePropertyShape ;
  sh:property fnscloud:SampleTypePropertyShape ;
  sh:property
  [
      sh:name "Secondary identifier" ;
      sh:description "Other identifier used for this study." ;
      sh:datatype xsd:string ;
      sh:path fnscloud:secondaryStudyIdentifier ;
  ],
  [
      sh:name "Subject count" ;
      sh:description "Number of subjects in the study." ;
      sh:datatype xsd:integer ;
      sh:maxCount 1 ;
      sh:path fnscloud:subjectCount ;
  ],
  [
      sh:name "Authors" ;
      sh:description "Authors" ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:path fnscloud:authors ;
  ],
  [
      sh:name "Pubmed id" ;
      sh:description "Pubmed id" ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:path fnscloud:pubmedId ;
  ],
  [
      sh:name "Pubmed central id" ;
      sh:description "Pubmed central id" ;
      sh:datatype xsd:string ;
      sh:maxCount 1 ;
      sh:path fnscloud:pubmedCentralId ;
  ],
  [
      sh:name "Year" ;
      sh:description "Year of publication" ;
      sh:datatype xsd:integer ;
      sh:maxCount 1 ;
      sh:path fnscloud:publishedYear ;
  ],
  [
      sh:name "Data in ENA" ;
      sh:description "Indicates whether study has data available in ENA" ;
      sh:datatype xsd:boolean ;
      sh:maxCount 1 ;
      sh:path fnscloud:isEnaDataAvailable ;
  ],
  [
      sh:name "Data in MGnify" ;
      sh:description "Indicates whether study has data available in MGnify" ;
      sh:datatype xsd:boolean ;
      sh:maxCount 1 ;
      sh:path fnscloud:isMgnifyDataAvailable ;
  ],
  [
      sh:name "Food intake available" ;
      sh:description "Indicates whether study has food intake data" ;
      sh:datatype xsd:boolean ;
      sh:maxCount 1 ;
      sh:path fnscloud:isFoodIntakeAvailable ;
  ],
  [
        sh:name "Food intake file" ;
        sh:description "Url to the food intake file" ;
        sh:path fnscloud:hasFoodIntakeFile ;
        fs:externalLink true ;
  ],
  [
      sh:name "Glucose monitoring available" ;
      sh:description "Indicates whether study has glucose monitoring data" ;
      sh:datatype xsd:boolean ;
      sh:maxCount 1 ;
      sh:path fnscloud:isGlucoseMonitorDoiAvailable ;
  ],
  [
      sh:name "Glucose monitor doi" ;
      sh:description "Reference to glucose monitor data at Zenodo" ;
      sh:datatype xsd:string ;
      sh:path fnscloud:hasGlucoseMonitorDoi ;
  ],
  [
      sh:name "Nutritional intake available" ;
      sh:description "Indicates whether study has nutritional intake data" ;
      sh:datatype xsd:boolean ;
      sh:path fnscloud:isNutritionalIntakeAvailable ;
  ],
  [
        sh:name "Nutritional intake file" ;
        sh:description "Url to the nutritional intake file" ;
        sh:path fnscloud:hasNutritionalIntakeFile ;
        fs:externalLink true ;
  ],
  [
      sh:path [sh:inversePath fnscloud:aboutStudy] ;
      sh:description "Linked files" ;
      sh:name "Files" ;
  ],
  [
      sh:path [sh:inversePath fnscloud:isGroupPartOfStudy] ;
      sh:description "Linked groups" ;
      sh:name "Groups" ;
  ],
  [
      sh:path [sh:inversePath fnscloud:isSubjectPartOfStudy] ;
      sh:description "Linked subjects" ;
      sh:name "Subjects" ;
  ],
  [
      sh:path [sh:inversePath fnscloud:isSamplePartOfStudy] ;
      sh:description "Linked samples" ;
      sh:name "Samples" ;
  ],
  [
      sh:path [sh:inversePath fnscloud:isAssayPartOfStudy] ;
      sh:description "Linked assays" ;
      sh:name "Assays" ;
  ].

fnscloud:isGroupPartOfStudy a rdf:Property .

fnscloud:Group
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:description "Group." ;
  sh:name "Group" ;
  sh:ignoredProperties (
      rdf:type
      owl:sameAs
    ) ;
  sh:property fnscloud:LabelPropertyShape ;
  sh:property fnscloud:DescriptionPropertyShape ;
  sh:property fnscloud:IdentifierPropertyShape ;
  sh:property fnscloud:IsDefinedByPropertyShape ;
  sh:property [
      sh:path fnscloud:isGroupPartOfStudy ;
      sh:class fnsharmony:study ;
      sh:description "The study this group belongs to." ;
      sh:name "Study" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:aboutGroup ;
        ] ;
      sh:description "Linked files" ;
      sh:name "Files" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:isPartOfGroup ;
        ] ;
      sh:description "Linked subjects" ;
      sh:name "Subjects" ;
    ] ;
.

fnscloud:Gender a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "The gender of the subject." ;
    sh:name "Gender" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

fnscloud:Species a rdfs:Class, sh:NodeShape ;
