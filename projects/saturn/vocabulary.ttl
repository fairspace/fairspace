    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

fnscloud:Ethnicity a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "The ethnicity of the subject." ;
    sh:name "Ethnicity" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property fnscloud:LabelPropertyShape ;
.

fnscloud:isOfGender a rdf:Property .
fnscloud:isOfSpecies a rdf:Property .
fnscloud:isOfEthnicity a rdf:Property .
fnscloud:isSubjectPartOfStudy a rdf:Property .
fnscloud:isPartOfGroup a rdf:Property .
fnscloud:hasAge a rdf:Property .
fnscloud:hasHeight a rdf:Property .
fnscloud:hasBmi a rdf:Property .

fnscloud:Subject
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:description "A subject is an object that is subject to instances of observation and measurement. For example in a study each living entity (animal or plant), bioreactor or area studied is one subject. When studying 50 different animals, each animal should become one subject." ;
  sh:name "Subject" ;
  sh:ignoredProperties (
      rdf:type
      owl:sameAs
    ) ;
  sh:property fnscloud:LabelPropertyShape ;
  sh:property fnscloud:IdentifierPropertyShape ;
  sh:property fnscloud:IsDefinedByPropertyShape ;
  sh:property fnscloud:GenderPropertyShape ;
  sh:property [
        sh:name "Species" ;
        sh:description "The species of the subject." ;
        sh:maxCount 1 ;
        sh:class fnscloud:Species ;
        sh:path fnscloud:isOfSpecies
    ] ;
  sh:property [
        sh:name "Age" ;
        sh:description "Subject age in years" ;
        sh:maxCount 1 ;
        sh:datatype xsd:integer ;
        sh:path fnscloud:hasAge
    ] ;
  sh:property [
    sh:name "BMI" ;
    sh:description "Body-mass index" ;
    sh:maxCount 1 ;
    sh:datatype xsd:decimal ;
    sh:path fnscloud:hasBmi
    ] ;
  sh:property [
        sh:name "Height" ;
        sh:description "Subject height in meters" ;
        sh:maxCount 1 ;
        sh:datatype xsd:decimal ;
        sh:path fnscloud:hasHeight
    ] ;
  sh:property fnscloud:DescriptionPropertyShape ;
  sh:property [
      sh:path fnscloud:isPartOfGroup ;
      sh:class fnscloud:Group ;
      sh:description "The group this subject belongs to." ;
      sh:name "Group" ;
    ] ;
  sh:property [
      sh:path fnscloud:isSubjectPartOfStudy ;
      sh:class fnsharmony:study ;
      sh:description "The study this subject belongs to." ;
      sh:name "Study" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:aboutSubject ;
        ] ;
      sh:description "Linked files" ;
      sh:name "Files" ;
    ] ;
  sh:property [
      sh:path [
          sh:inversePath fnscloud:belongsToSubject ;
        ] ;
      sh:description "Linked samples" ;
      sh:name "Samples" ;
    ] ;
.

