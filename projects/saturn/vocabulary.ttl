@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://www.schema.org/> .

@prefix fs: <https://fairspace.nl/ontology#> .

fs:a001 a rdf:Property .
fs:a002 a rdf:Property .
fs:a003 a rdf:Property .
fs:a004 a rdf:Property .
fs:a005 a rdf:Property .
fs:hasParent a rdf:Property .
fs:hasTreatmentType rdf:type rdf:Property .

fs:s001 a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Object s001 - root" ;
    sh:name "S001" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Attribute001" ;
        sh:description "Attribute 001" ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a001
    ],
    [
      sh:path [sh:inversePath fs:hasParent] ;
      sh:description "Is parent of" ;
      sh:name "Parent" ;
    ];
.
fs:s002 a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Object s002 - root." ;
    sh:name "S002" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Attribute002" ;
        sh:description "Attribute 002." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a002
    ],
    [
        sh:path fs:hasParent ;
        sh:class fs:s001 ;
        sh:description "Has parent" ;
        sh:name "Child" ;
    ],
    [
      sh:path [sh:inversePath fs:hasParent] ;
      sh:description "Is parent of" ;
      sh:class fs:s003 ;
      sh:name "Parent" ;
    ];
.
fs:s003 a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Object s003 - root." ;
    sh:name "S003" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Attribute003" ;
        sh:description "Attribute 003." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a003
    ],
    [
        sh:name "Attribute005" ;
        sh:description "Attribute 005." ;
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
        sh:path fs:a005
    ],
    [
        sh:path fs:hasParent ;
        sh:class fs:s002 ;
        sh:description "Has parent" ;
        sh:name "Child" ;
    ],
    [
      sh:path [sh:inversePath fs:hasParent] ;
      sh:description "Is parent of" ;
      sh:class fs:s004 ;
      sh:name "Parent" ;
    ];

.
fs:s004 a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Object s004 - root." ;
    sh:name "S004" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
    sh:property
    [
        sh:name "Attribute001" ;
        sh:description "Attribute 001." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a001
    ],
    [
        sh:name "Attribute002" ;
        sh:description "Attribute 002." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a002
    ],
    [
        sh:name "Attribute003" ;
        sh:description "Attribute 003." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a003
    ],
    [
        sh:name "Attribute004" ;
        sh:description "Attribute 004." ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:path fs:a004
    ],
    [
        sh:path fs:hasParent ;
        sh:class fs:s003 ;
        sh:description "Has parent" ;
        sh:name "Child" ;
    ],
    [
        sh:path fs:hasTreatmentType ;
        sh:class fs:treatmentType ;
        sh:description "The type of treatment, e.g. fasting, OLTT." ;
        sh:name "Treatment type" ;
    ];
.

fs:treatmentType a rdfs:Class, sh:NodeShape ;
    sh:closed false ;
    sh:description "Treatment type, e.g. screening, diclofenac, placebo, etc" ;
    sh:name "Treatment type" ;
    sh:ignoredProperties ( rdf:type owl:sameAs ) ;
.
